{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.4.0"
    }
  },
  "rewrites": [
    { "source": "/favicon.ico", "destination": "https://juicy.vercel.app/favicon.ico" },
    { "source": "/(.*)", "destination": "/api/index.php" },
    { "source": "/index.php", "destination": "/index.php" }
  ]
}
